<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="thumbnail">

    <select id="selectList" resultType="ThumbnailOption">
        SELECT
          `pn`, `width`, `height`, `type`, `mprType`, `quality`,
          `brightness`, `density`, `transferOffset`,
          `transferScaleX`, `transferScaleY`, `transferScaleZ`,
          `positionZ`, `rotationX`, `rotationY`
        FROM
          `thumbnail_option`
    </select>

</mapper>
